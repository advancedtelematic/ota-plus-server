{
    "extends": "tslint-config-airbnb",
    "rules": {
        "import-name": false,
        "max-line-length": [
            true,
            {
                "limit": 120
            }
        ],
        "trailing-comma": false,
        "variable-name": [
            true,
            "ban-keywords",
            "check-format",
            "allow-pascal-case"
        ]
    }
}
