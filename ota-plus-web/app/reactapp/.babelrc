{
  "presets": [
    "@babel/preset-env",
    "@babel/preset-react"
  ],
  "plugins": [
    [
      "@babel/plugin-proposal-decorators",
      {
        "legacy": true
      }
    ],
    [
      "@babel/plugin-proposal-class-properties",
      {
        "loose": true
      }
    ],
    [
      "import",
      {
        "libraryName": "antd",
        "libraryDirectory": "lib"
      }
    ],
    [
      "import",
      {
        "libraryName": "formsy-antd"
      },
      "ant-formsy"
    ],
    [
      "@babel/transform-runtime"
    ]
  ]
}