    "identity-obj-proxy": "3.0.0",
    "jest": "24.9.0",
    "jest-transform-stub": "2.0.0",
    "less": "3.9.0",
    "less-loader": "4.1.0",
    "line-reader": "0.4.0",
    "node-sass": "4.12.0",
    "path": "0.12.7",
    "prettier": "1.17.0",
    "resolve-url-loader": "3.1.0",
    "sass-loader": "7.1.0",
    "style-loader": "0.23.1",
    "unzip": "0.1.11",
    "url-loader": "1.1.2",
    "walk": "2.3.14",
    "webpack": "4.30.0",
    "webpack-cli": "3.3.0",
    "webpack-dev-server": "3.3.1"
  },
  "jest": {
    "moduleDirectories": [
      "node_modules"
    ],
    "moduleNameMapper": {
      "^.+.(css|styl|less|sass|scss|png|jpg|ttf|woff|woff2)$": "jest-transform-stub"
    },
    "setupFiles": [
      "./setupTests.js"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "transform": {
      "^.+\\.(t|j)sx?$": "babel-jest",
      "^.+\\.html?$": "html-loader-jest",
      ".+\\.(css|styl|less|sass|scss|png|jpg|ttf|woff|woff2)$": "jest-transform-stub"
    },
    "transformIgnorePatterns": [
      "/node_modules/antd/(?!es)"
    ],
    "verbose": true
  },
  "scripts": {
    "dev": " ./node_modules/webpack/bin/webpack.js -w",
    "izumi-transform": "node scripts/izumiTextManager/izumiTextManager.js",
    "lint": "eslint src/** --ext .js,.jsx",
    "npm-check": "npm-check -p",
    "socket-rome": "curl -k https://web-events.rome.sit-ota.aws.in.here.com/",
    "test": "set NODE_ENV=test && jest",
    "test:coverage": "set NODE_ENV=test && jest --coverage --collectCoverageFrom=src/**/*.{js,jsx}",
    "translations-check": "node tests/translations_check.js"
  },
  "keywords": [
    "atsgarage"
  ],
  "author": "ATS"
}
