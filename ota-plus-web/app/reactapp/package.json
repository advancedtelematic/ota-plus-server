    "antd": "3.15.2",
    "axios": "0.19.0",
    "chart.js": "2.8.0",
    "encodeurl": "1.0.2",
    "form-serialize": "0.7.2",
    "formsy-antd": "1.1.0",
    "formsy-react": "1.1.5",
    "i18next": "18.0.0",
    "i18next-browser-languagedetector": "3.0.1",
    "i18next-xhr-backend": "3.0.0",
    "jquery": "3.4.0",
    "js-cookie": "2.2.0",
    "lodash": "4.17.15",
    "mobx": "5.15.0",
    "mobx-react": "6.1.4",
    "moment": "2.24.0",
    "normalize.css": "8.0.1",
    "prop-types": "15.7.2",
    "react": "16.8.6",
    "react-animate-height": "2.0.15",
    "react-chartjs-2": "2.7.6",
    "react-click-outside": "3.0.1",
    "react-copy-to-clipboard": "5.0.1",
    "react-dnd": "7.4.5",
    "react-dnd-html5-backend": "7.4.4",
    "react-dom": "16.8.6",
    "react-dropzone": "7.0.1",
    "react-i18next": "11.0.0",
