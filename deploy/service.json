[{
  "id": "${DEPLOY_ENV}/${JOB_NAME}",
  "uris": ["/root/docker.tar.gz"],
  "constraints": [["rack_id", "CLUSTER", "$MARATHON_CONSTRAINTS"]],
  "cpus": 0.2,
  "mem": 1024,
  "instances": 1,
  "args": ["-Dhttp.port=9001"],
  "container": {
    "type": "DOCKER",
    "docker": {
      "image": "${IMAGE_ARTIFACT}",
      "parameters": [{"key": "log-driver", "value": "syslog"},
                     {"key": "log-opt", "value": "tag=${SERVICE_NAME}"}],
      "forcePullImage": true,
      "network": "BRIDGE",
      "portMappings": [{
        "containerPort": 9001,
        "hostPort": 0,
        "protocol": "tcp"
      }]
    }
  },
  "env": {
    "AUDITOR_HOST": "${AUDITOR_HOST}",
    "AUDITOR_PORT": "${AUDITOR_PORT}",
    "AUTH0_AUTH_PLUS_CLIENT_ID": "${AUTH0_AUTH_PLUS_CLIENT_ID}",
    "AUTH0_CALLBACK_URL": "${AUTH0_CALLBACK_URL}",
    "AUTH0_CLIENT_ID": "${AUTH0_CLIENT_ID}",
    "AUTH0_CLIENT_SECRET": "${AUTH0_CLIENT_SECRET}",
    "AUTH0_DB_CONNECTION": "${AUTH0_DB_CONNECTION}",
    "AUTH0_DOMAIN": "${AUTH0_DOMAIN}",
    "AUTH0_USER_UPDATE_TOKEN": "${AUTH0_USER_UPDATE_TOKEN}",
    "AUTH0_SIGN_UP_URL": "${AUTH0_SIGN_UP_URL}",
    "AUTHPLUS_CLIENT_ID": "${AUTHPLUS_CLIENT_ID}",
    "AUTHPLUS_SECRET": "${AUTHPLUS_SECRET}",
    "AUTHPLUS_TOKEN_VERIFY": "true",
    "AUTH_PLUS_HOST": "${AUTH_PLUS_HOST}",
    "AUTH_PLUS_PORT": "${AUTH_PLUS_PORT}",
    "AUTH_PLUS_SCHEME": "https",
    "BUILDER_HOST": "${BUILDER_HOST}",
    "BUILDER_PORT": "${BUILDER_PORT}",
    "CAMPAIGNER_HOST": "${CAMPAIGNER_HOST}",
    "CAMPAIGNER_PORT": "${CAMPAIGNER_PORT}",
    "CRYPT_HOST": "${CRYPT_HOST}",
    "CRYPT_PORT": "${CRYPT_PORT}",
    "DEVICE_GATEWAY_PORT": "${DEVICE_GATEWAY_PORT}",
    "DEVICE_REGISTRY_HOST": "${DEVICE_REGISTRY_HOST}",
    "DEVICE_REGISTRY_PORT": "${DEVICE_REGISTRY_PORT}",
    "DIRECTOR_HOST": "${DIRECTOR_HOST}",
    "DIRECTOR_PORT": "${DIRECTOR_PORT}",
    "JAVA_OPTS": "${JAVA_OPTS}",
    "JWT_SECRET": "${JWT_SECRET}",
    "KAFKA_HOST": "${KAFKA_HOST}",
    "KAFKA_TOPIC_SUFFIX": "${KAFKA_TOPIC_SUFFIX}",
    "MESSAGING_MODE": "kafka",
    "OIDC_CONFIG_URL": "${OIDC_CONFIG_URL}",
    "OIDC_ISSUER": "${OIDC_ISSUER}",
    "PACKAGES_VERSION_FORMAT": "${PACKAGES_VERSION_FORMAT}",
    "PLAY_CRYPTO_SECRET": "${PLAY_CRYPTO_SECRET}",
    "SERVICE_HEALTH_CHECK": "curl --show-error --silent %<host>s:%<port>s/health",
    "SERVICE_IMAGE": "${IMAGE_ARTIFACT}",
    "SERVICE_NAME": "${SERVICE_NAME}",
    "SERVICE_SCOPE": "public",
    "SERVICE_TYPE": "websockets",
    "SOTA_CORE_HOST": "${SOTA_CORE_HOST}",
    "SOTA_CORE_PORT": "${SOTA_CORE_PORT}",
    "TREEHUB_HOST_PUB": "${TREEHUB_HOST_PUB}",
    "TREEHUB_PORT_PUB": "${TREEHUB_PORT_PUB}",
    "TREEHUB_SCHEME_PUB": "https",
    "TUF_KEYSERVER_HOST": "${TUF_KEYSERVER_HOST}",
    "TUF_KEYSERVER_PORT": "${TUF_KEYSERVER_PORT}",
    "TUF_REPOSERVER_HOST": "${TUF_REPOSERVER_HOST}",
    "TUF_REPOSERVER_PORT": "${TUF_REPOSERVER_PORT}",
    "TUF_REPOSERVER_HOST_PUB": "${TUF_REPOSERVER_HOST_PUB}",
    "TUF_REPOSERVER_PORT_PUB": "${TUF_REPOSERVER_PORT_PUB}",
    "TUF_REPOSERVER_SCHEME_PUB": "https",
    "USER_PROFILE_HOST": "${USER_PROFILE_HOST}",
    "USER_PROFILE_PORT": "${USER_PROFILE_PORT}",
    "WS_HOST": "${WS_HOST}",
    "WS_PORT": "${WS_PORT}"
  },
  "healthChecks": [{
    "protocol": "HTTP",
    "path": "/health",
    "intervalSeconds": 30
  }]
},
